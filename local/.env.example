# Auth0 Configuration
AUTH0_ISSUER_URI=https://your-domain.auth0.com/
AUTH0_AUDIENCE=https://your-domain.com/api
AUTH0_DOMAIN=your-domain.auth0.com
AUTH0_TOKEN_URI=https://your-domain.auth0.com/oauth/token

# Auth0 Management API (for Authorization Service)
AUTH0_MGMT_CLIENT_ID=your_management_client_id
AUTH0_MGMT_CLIENT_SECRET=your_management_client_secret

# Language Service M2M
LANGUAGE_M2M_CLIENT_ID=your_language_m2m_client_id
LANGUAGE_M2M_CLIENT_SECRET=your_language_m2m_client_secret

# Snippet Service M2M
SNIPPET_SERVICE_M2M_CLIENT_ID=your_snippet_m2m_client_id
SNIPPET_SERVICE_M2M_CLIENT_SECRET=your_snippet_m2m_client_secret

# Database - Language Service
POSTGRES_USER_LANGUAGE=language_user
POSTGRES_PASSWORD_LANGUAGE=language_password
POSTGRES_DB_LANGUAGE=language_db

# Database - Authorization Service
POSTGRES_USER_AUTHORIZATION=authorization_user
POSTGRES_PASSWORD_AUTHORIZATION=authorization_password
POSTGRES_DB_AUTHORIZATION=authorization_db

# Database - Snippet Service
POSTGRES_USER_SNIPPET=snippet_user
POSTGRES_PASSWORD_SNIPPET=snippet_password
POSTGRES_DB_SNIPPET=snippet_db

# UI Environment Variables
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_ui_client_id
VITE_AUDIENCE=https://your-domain.com/api
VITE_DOMAIN=localhost

# New Relic (optional)
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key_here